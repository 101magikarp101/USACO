size(2000,1500);
import fontsize;
defaultpen(fontsize(90pt));
pen p = 5+black;
real s = 11+sqrt(3);
pair A = (0,0), B = (s,0), C = (s/2,s*sqrt(3)/2), D = (s/2,0), E = (s/2,s*sqrt(3)/6);
pair E = (2.5,2.5*sqrt(3));
pair G = (11,0);
pair F = (6+2.5,2.5*sqrt(3)+3);
pair O = (6+2.5,2.5*sqrt(3));
draw(A--B--C--cycle,p);
draw(G--O--E,p);
draw(O--F,p);
label("$6$",(6,2.5*sqrt(3)),N);
label("$3$",(8.5,2.5*sqrt(3)+1.5),W);
label("$5$",(6+5*.75,1.25*sqrt(3)),SW);
label("$A$",A,SW);
label("$B$",B,SE);
label("$C$",C,N);
label("$D$",D,S);
label("$E$",E,N+(-0.8,-0.8));
label("$F$",F,N+(0.55,0));
label("$G$",G,S);
label("$O$",O,NE);